{
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module",
    "ecmaFeatures": {
      "modules": true,
      "experimentalObjectRestSpread": true
    }
  },
  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "strict": 0,
    "semi": 0,
    "import/extensions": 0,
    "no-restricted-syntax": 0
  },
  "env": {
    "browser": true
  }
}
